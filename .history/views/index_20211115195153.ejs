<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Budgtr</title>
    

    

    <link rel="stylesheet" href="/css/normalize.css" />
    <link rel="stylesheet" href="/css/skeleton.css" />
</head>
<body>
  
      <h1>Budget</h1>

      <table class="u-full-width">

      <tbody>



  <tr>
              
              <td>  <%= budget[0].date %></td>
              <td>  <a href="/budget/<%=0%>"> <%= budget[0].name %></a> </td>
              <td>  <%= budget[0].amount %>  </td>
         
       </tr>

       <tr>
          
          <td>  <%= budget[1].date %></td>
          <td>  <a href="/budget/<%=1%>"> <%= budget[1].name %></a> </td>
          <td>  <%= budget[1].amount %>  </td>
      
     
   </tr>  
  
   <tr>
      
      <td>  <%= budget[2].date %></td>
      <td>  <a href="/budget/<%=2%>"> <%= budget[2].name %></a> </td>
      <td>  <%= budget[2].amount %>  </td>
   
 


  


<table class="u-full-width">

  <tbody>

<% for(let i = 0; i < budget.length; i++) { %>

  <tr>
    
    <td>  <%= budget[i].date %></td>
    <td>  <a href="/budget/<%=i%>"> <%= budget[i].name %></a> </td>
    <td>  <%= budget[i].amount %>  </td>

</tr>

  
      
<% } %>
    

</tbody>
</table>

   
</body>
</html>